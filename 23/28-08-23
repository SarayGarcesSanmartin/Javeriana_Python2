#Punto 1 parcial#
def countDNA(dnastring):
    letra_A = 0
    letra_T = 0
    letra_C = 0
    letra_G = 0
    for char in adn:
        if char == "A":
            letra_A += 1
        elif char == "T":
            letra_T += 1
        elif char == "C":
            letra_C += 1
        elif char == "G":
            letra_G += 1
    print("Cantidad de A:", letra_A, "Cantidad de T:", letra_T, "Cantidad de C:", letra_C, "Cantidad de G:", letra_G)
    print("Cantidad total de bases en la cadena:",len(adn))
adn = input("Ingrese la cadena de ADN: ")

countDNA(adn)


